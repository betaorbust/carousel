{
	"name": "@betaorbust/carousel",
	"version": "1.0.0",
	"description": "Carousel",
	"homepage": "https://betaorbust.github.io/carousel",
	"keywords": [
		"typescript",
		"react",
		"carousel"
	],
	"main": "src/index.tsx",
	"dependencies": {
		"@emotion/react": "11.10.6",
		"@testing-library/jest-dom": "^5.16.5",
		"@types/tiny-slider-react": "^0.3.4",
		"loader-utils": "3.2.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-scripts": "5.0.1",
		"react-swipeable": "7.0.0",
		"tiny-slider": "^2.9.4",
		"tiny-slider-react": "^0.5.7",
		"usehooks-ts": "2.9.1"
	},
	"devDependencies": {
		"@betaorbust/eslint-config": "^4.0.1",
		"@commitlint/cli": "^17.5.0",
		"@commitlint/config-conventional": "^17.4.4",
		"@testing-library/react": "^14.0.0",
		"@types/react": "18.0.28",
		"@types/react-dom": "18.0.11",
		"eslint": "^8.36.0",
		"husky": "^8.0.3",
		"identity-obj-proxy": "^3.0.0",
		"lint-staged": "^13.2.0",
		"prettier": "^2.8.6",
		"typescript": "^4.9.5"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"test:debug": "react-scripts --inspect-brk test --env=jsdom --runInBand --no-cache",
		"eject": "react-scripts eject",
		"format": "prettier --loglevel warn --write \"**/*.{js,ts,jsx,tsx,mdx,vue,json,json5,yaml,md,html,less,sass,css,gql}\"",
		"prepare": "husky install",
		"lint": "eslint ./"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"lint-staged": {
		"*.{js,ts,jsx,tsx,mdx,vue,json,json5,yaml,md,html,less,sass,css,gql}": "prettier --write"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		],
		"rules": {
			"subject-case": [
				2,
				"always",
				[
					"sentence-case"
				]
			],
			"subject-full-stop": [
				2,
				"always",
				"."
			]
		}
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(css|less)$": "identity-obj-proxy"
		}
	}
}
